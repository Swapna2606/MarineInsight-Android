<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.marineinsight.android" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/mi_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.marineinsight.android.MainActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize">

        </activity>
        <activity
            android:name="com.marineinsight.android.SplashActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize">

        </activity>
        <activity
            android:name="com.marineinsight.android.DetailActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize">

        </activity>
        <activity
            android:name="com.marineinsight.android.OffDetailActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize">

        </activity>
        <activity
            android:name="com.marineinsight.android.OfflineActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize">

        </activity>
        <activity android:name="com.marineinsight.android.WelcomeActivity"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>
